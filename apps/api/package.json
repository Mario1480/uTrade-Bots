    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "nodemailer": "^6.10.1",
    "technicalindicators": "^3.1.0",
    "ws": "^8.18.3",
    "zod": "^3.23.8",
    "@mm/futures-core": "0.0.1"
